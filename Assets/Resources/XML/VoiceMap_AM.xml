<?xml version="1.0" encoding="utf-8"?>
<VoiceList xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>AirwayMD</Name>
	<VoiceMaps>
	    <VoiceMap>
	      <Tag>VOICE:BUSY</Tag>
	      <Audio>Audio/AM_Busy</Audio>
	      <Text>I am busy at the momemt</Text>
	    </VoiceMap>
	    <VoiceMap>
	      <Tag>VOICE:WRONG:PERSON</Tag>
	      <!--<Audio>Audio/AM_WrongPerson</Audio>-->
	      <Text>I don't know how to do that</Text>
	    </VoiceMap>
	    <VoiceMap>
	      <Tag>VOICE:EXCEEDED:MAX:INTERACTION</Tag>
	      <Text>I have already done that!</Text>
	    </VoiceMap>
	    <VoiceMap>
	      <Tag>VOICE:MISSING:PREREQ</Tag>
	      <Text>Can't do that right now, did you forget something?</Text>
	    </VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:01</Tag>
			<Audio>Audio/01_intubate_main</Audio>
	  		<Text>OK, you have the c-spine?</Text>
			<LookAt>ignore</LookAt>
	    </VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:03</Tag>
			<Audio>Audio/03_intubate_main</Audio>
			<Text>Go ahead and walk your hands up. I'm going to take the collar off.</Text>
			<LookAt>ignore</LookAt>
	    </VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:04</Tag>
			<Audio>Audio/04_intubate_main</Audio>
			<Text>Don't let me pull the neck away from you.</Text>
			<LookAt>ignore</LookAt>
	    </VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:05</Tag>
			<Audio>Audio/05_intubate_main</Audio>
			<Text>Is suction ready?</Text>
			<LookAt>ignore</LookAt>
	    </VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:07</Tag>
			<Audio>Audio/07_intubate_main</Audio>
			<Text>Do you have the tube ready?</Text>
			<LookAt>ignore</LookAt>
	    </VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:09</Tag>
			<Audio>Audio/09_intubate_main</Audio>
			<Text>End tidal CO2 is ready?</Text>
			<LookAt>ignore</LookAt>
	    </VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:11A</Tag>
			<Audio>Audio/11a_intubate_main</Audio>
			<Text>I'll take the laryngiscope</Text>
			<LookAt>ignore</LookAt>
	  	</VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:11B</Tag>
			<Audio>Audio/11b_intubate_main</Audio>
			<Text>ET tube, please.</Text>
			<LookAt>ignore</LookAt>
	    </VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:12</Tag>
			<Audio>Audio/12_intubate_main</Audio>
			<Text>OK, I'm in.</Text>
			<LookAt>ignore</LookAt>
	    </VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:12B</Tag>
			<Audio>Audio/AM_removeStylet</Audio>
			<Text>Remove the stylet.</Text>
			<LookAt>ignore</LookAt>
	    </VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:13</Tag>
			<Audio>Audio/13_intubate_main</Audio>
			<Text>Go ahead and inflate the cuff.</Text>
			<LookAt>ignore</LookAt>
	  	</VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:15</Tag>
			<Audio>Audio/15_intubate_main</Audio>
			<Text>OK, nice job everyone.</Text>
			<LookAt>ignore</LookAt>
	  	</VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:02F</Tag>
			<Audio>Audio/02_failIntubate_AM</Audio>
			<Text>No, I'm fine, just give me a second; I've almost got it.</Text>
			<LookAt>ignore</LookAt>
	  	</VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:04F</Tag>
			<Audio>Audio/04_failIntubate_AM</Audio>
			<Text>Yes, I'm sure.</Text>
			<LookAt>ignore</LookAt>
	  	</VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:05F</Tag>
			<Audio>Audio/05_failIntubate_AM</Audio>
			<Text>Is she sedated? I can't get a good line of sight.</Text>
			<LookAt>ignore</LookAt>
	  	</VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:07F</Tag>
			<Audio>Audio/07_failIntubate_AM</Audio>
			<Text>She's moving a lot. You ordered etimodate right?</Text>
			<LookAt>ignore</LookAt>
	  	</VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:08F</Tag>
			<Audio>Audio/08_failIntubate_AM</Audio>
			<Text>I need suction now! Keep the neck stable.</Text>
			<LookAt>ignore</LookAt>
	  	</VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:10F</Tag>
			<Audio>Audio/10_failIntubate_AM</Audio>
			<Text>Take the ET tube, I've got suction.</Text>
			<LookAt>ignore</LookAt>
	  	</VoiceMap>
		<VoiceMap>
			<Tag>VOICE:INTUBATE:11F</Tag>
			<Audio>Audio/11_failIntubate_AM</Audio>
			<Text>This isn't going to happen. Collar's going back on.</Text>
			<LookAt>ignore</LookAt>
	  	</VoiceMap>
    <VoiceMap>
      <Tag>VOICE:INTUBATE:VOMIT</Tag>
      <Audio>Audio/AM_intubateVomit</Audio>
      <Text>I can't see anything. The patient has vomited. The airway is blocked.</Text>
	  <LookAt>ignore</LookAt>
    </VoiceMap>
<VoiceMap>
      <Tag>VOICE:PATIENT:VOMITED</Tag>
      <Audio>Audio/AM_patientVomited</Audio>
      <Text>The patient has vomited. The airway is blocked.</Text>
    </VoiceMap>
    <!-- Dynamic Vent Connect-->
    <VoiceMap>
      <Tag>VOICE:VENT:REPORT40</Tag>
	  <Dynamic>true</Dynamic>
      <Audio>audio=SAY:VENT:SET number=40 audio=SAY:VITALS:PERCENT audio=SAY:VENT:RATE </Audio>
    </VoiceMap>
    <VoiceMap>
      <Tag>SAY:VENT:SET</Tag>
      <Audio>Audio/AM_ventConnectSet</Audio>
      <Text>The ventilator is now connected, Doctor. The vent is set at FIO2 </Text>
      <LookAt>lookRT</LookAt>
    </VoiceMap>
    <VoiceMap>
      <Tag>SAY:VENT:RATE</Tag>
      <Audio>Audio/AM_ventConnectRate</Audio>
      <Text>Rate 14, TV 550, Peep plus 5.</Text>
      <LookAt>lookRT</LookAt>
    </VoiceMap>
    <VoiceMap>
      <Tag>VOICE:VENT:REPORT</Tag>
	  <Dynamic>true</Dynamic>
      <Audio>audio=SAY:VENT:SET number=100 audio=SAY:VITALS:PERCENT audio=SAY:VENT:RATE</Audio>
    </VoiceMap>
    <!-- Dynamic Vent Connect END-->
    <VoiceMap>
      <Tag>VOICE:VENT:CONFIRM</Tag>
      <Audio>Audio/AM_thatsCorrect</Audio>
      <Text>That's correct.</Text>
      <LookAt>lookRT</LookAt>
    </VoiceMap>
    <VoiceMap>
      <Tag>VOICE:PREP:INTUBATION</Tag>
      <Audio>Audio/AM_iAgree</Audio>
      <Text>I agree, she needs to be intubated. Do you want to give any medications?</Text>
    </VoiceMap>
    <VoiceMap>
      <Tag>VOICE:BREATHE:RESULTS</Tag>
      <Audio>Audio/AM_breatheResults</Audio>
      <Text>Breath sounds are good bilaterly. Chest wall motion is equal bilaterally.</Text>
    </VoiceMap>
    <VoiceMap>
      <Tag>VOICE:PERCUSSION:REPORT</Tag>
      <Audio>Audio/AM_percussionNormal</Audio>
      <Text>Percussion is normal, Doctor.</Text>
      <LookAt>lookPatient</LookAt>
    </VoiceMap>
    <VoiceMap>
      <Tag>VOICE:CHECK:ETCO2</Tag>
      <Audio>Audio/AM_iHaveGoodColor</Audio>
      <Text>I have good color change and fogging in the tube. We are 22 at the lip.</Text>
      <LookAt>lookPatient</LookAt>
    </VoiceMap>
    <VoiceMap>
      <Tag>VOICE:PALPATETRACHEA:REPORT</Tag>
      <Audio>Audio/AM_tracheaMidline</Audio>
      <Text>The trachea is midline. No obvious abnormalities.</Text>
      <LookAt>lookCamera</LookAt>
    </VoiceMap>
    <VoiceMap>
      <Tag>VOICE:BREATHE:REPORT</Tag>
      <Audio>Audio/AM_checkBreathe</Audio>
      <Text>Breath sounds are good bilaterly.</Text>
      <LookAt>lookCamera</LookAt>
    </VoiceMap>
    <VoiceMap>
      <Tag>VOICE:AUSCULTATEHEART:REPORT</Tag>
      <Audio>Audio/AM_heartSoundsNormal</Audio>
      <Text>Heart sounds are normal.</Text>
      <LookAt>lookCamera</LookAt>
    </VoiceMap>
    <VoiceMap>
        <Tag>VOICE:LOGROLL:COUNT</Tag>
        <Audio>Audio/AM_logRollCount</Audio>
        <Text>OK Everyone. On my count. One. Two. Three.</Text>
        <LookAt>lookPatient</LookAt>
    </VoiceMap>
    <VoiceMap>
        <Tag>VOICE:LOGROLL:FINISH</Tag>
        <Audio>Audio/AM_logRollFinish</Audio>
        <Text>Down on three. One. Two. Three.</Text>
        <LookAt>lookPatient</LookAt>
    </VoiceMap>
