<?xml version="1.0" encoding="utf-8"?>
<ArrayOfVideoTriggerInfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <VideoTriggerInfo>
	<Name>MISSING SPLINT</Name>
	<Case>T2</Case>
	<Text>This event was triggered because you didn't put a splint on your patient after 10 minutes</Text>
	<Interactions>
		<string>PERFORM:SPLINT after=10</string>
	</Interactions>
	<Key>Splints</Key>
	<Movie>Debriefing/Splints.ogv</Movie>
	<Thumbnail>VideoStills/vidStill_splints_thumb</Thumbnail>
  </VideoTriggerInfo>
  
  <VideoTriggerInfo>
  	<Name>DELIVERING RSI MEDS</Name>
	<Case>T4,T5</Case>
    <Text>This event was triggered because you didn't use rapid sequence medications when intubating your patient.</Text>
    <Interactions>
      <string>INTUBATE:PATIENT:COMPLETE</string>
      <string>!ADMINSTER:RSIMEDS</string>
    </Interactions>
	<Key>RSI</Key>
    <Movie>Debriefing/RapidSequenceIntubation.ogv</Movie>
    <Thumbnail>VideoStills/vidStill_rapidSeqIntub_thumb</Thumbnail>
  </VideoTriggerInfo>
  
  <VideoTriggerInfo>
  	<Name>BREATHING BEFORE AIRWAY</Name>
    <Text>This event was triggered because you listened to breath sounds before clearing airway OR two minutes has passed and you haven't cleared airway.</Text>
    <PrereqOperator>or</PrereqOperator>
    <Prereqs>
  	  <string>PATIENT:GCS</string>
      <string>PLAYER:ASKNAME</string>
      <string>PLAYER:ASKCAUSE</string>
      <string>PLAYER:ASKPAIN </string>
    </Prereqs>
    <Interactions>
      <string>CHECK:BREATHE after=120</string>
    </Interactions>
	<Key>Prioritization</Key>
    <Movie>Debriefing/Prioritization.ogv</Movie>
    <Thumbnail>VideoStills/vidStill_prioritization_thumb</Thumbnail>
  </VideoTriggerInfo>

    <VideoTriggerInfo>
 	<Name>CIRCULATION BEFORE BREATHING</Name>
    <Text>This event was triggered because you checked Circulation before getting a saturated oxygen reading.</Text>
	<PrereqOperator>or</PrereqOperator>
 	<Prereqs>
 	  <string>VITALS:SAO2</string>
 	  <string>VITALS:ALL</string>
	</Prereqs>
 	<InteractionOperator>or</InteractionOperator>
    <Interactions>
      <string>PATIENT:SKINTEMP</string>
      <string>PATIENT:CAPREFILL</string>
      <string>PULSE:FEMORAL</string>
      <string>PULSE:CAROTID</string>
      <string>PULSE:RADIAL</string>
      <string>EXAM:BLEEDING</string>
    </Interactions>
	<Key>Prioritization</Key>
 	<Movie>Debriefing/Prioritization.ogv</Movie>
    <Thumbnail>VideoStills/vidStill_prioritization_thumb</Thumbnail>
  </VideoTriggerInfo>
  
  <VideoTriggerInfo>
  	<Name>DELAYED INTUBATION</Name>
	<Case>T3,T4,T5</Case>
    <Text>This video was triggered because over a minute has passed without requesting intubation in a patient with a GCS of 9 or below.</Text>
	<TimedCondition>true</TimedCondition>
	<Conditions>
      <string>%GCS_TOTAL_FLOAT less 9</string>
    </Conditions>
    <Interactions>
      <string>INTUBATE:PATIENT after=60</string>
    </Interactions>
	<Key>Delayed Intubation</Key>
    <Movie>Debriefing/DelayedIntubation.ogv</Movie>
    <Thumbnail>VideoStills/vidStill_delayedIntubation_thumb</Thumbnail>
  </VideoTriggerInfo>
  
  <VideoTriggerInfo>
  	<Name>NO SECONDARY IV</Name>
    <Text>This event was triggered because you did not provide two IVs for fluid resuscitation.</Text>
 	<Prereqs>
 	  <string>!IVPORT:RIGHT</string>
	</Prereqs>
	<InteractionOperator>or</InteractionOperator>
		<Interactions>
			  <string>PLAYER:PATIENT:ADMIT</string>
		 	  <string>PLAYER:PATIENT:ADMIT:ICU</string>
		 	  <string>PLAYER:PATIENT:DISCHARGE</string>
		 	  <string>CALL:OR</string>
		 	  <string>CALL:IR</string>
		</Interactions>
	<Key>Resuscitation</Key>
    <Movie>Debriefing/HowMuchFluid.ogv</Movie>
    <Thumbnail>VideoStills/vidStill_delayedResus_thumb</Thumbnail>
  </VideoTriggerInfo>

  <VideoTriggerInfo>
  	<Name>DELAYED RESUSCITATION AFTER 2 MINUTES</Name>
    <Text>This event was triggered because you did not provide fluid resuscitation to your patient after 2 minutes of a systolic blood pressure lower than 110.</Text>
 	<Prereqs>
 	  <string>%BP_SYS less 110</string>
	</Prereqs>
    <InteractionOperator>or</InteractionOperator>
		<Interactions>
			<string>ORDER:CRYSTAL:1IV after=120</string>
			<string>ORDER:CRYSTAL:2IV after=120</string>
			<string>ORDER:CRYSTAL:1PR after=120</string>
			<string>ORDER:CRYSTAL:2PR after=120</string>
			<string>ORDER:CRYSTAL:1RI after=120</string>
			<string>ORDER:CRYSTAL:2RI after=120</string>
			<string>ORDER:PRBC:1IV after=120</string>
			<string>ORDER:PRBC:2IV after=120</string>
			<string>ORDER:PRBC:1PR after=120</string>
			<string>ORDER:PRBC:2PR after=120</string>
			<string>ORDER:PRBC:1RI after=120</string>
			<string>ORDER:PRBC:2RI after=120</string>
		</Interactions>
	<Key>Resuscitation</Key>
    <Movie>Debriefing/HowMuchFluid.ogv</Movie>
    <Thumbnail>VideoStills/vidStill_delayedResus_thumb</Thumbnail>
  </VideoTriggerInfo>
  
