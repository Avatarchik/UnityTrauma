      <Name>Recovering</Name>
      <Transitions>
        <DecisionStateTransition>
          <DecisionCondition>
            <Name>TEST SP</Name>
            <Tests>
              <string>var:Patient.SP condition:greater constant:93</string>
            </Tests>
          </DecisionCondition>
          <DecisionAction>
            <Name>Action1</Name>
            <Actions />
          </DecisionAction>
          <SuccessState>Watching</SuccessState>
        </DecisionStateTransition>
