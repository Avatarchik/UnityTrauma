          <SuccessState>Recovering</SuccessState>
        </DecisionStateTransition>	  
        <DecisionStateTransition>
          <DecisionCondition>
            <Name>TEST SP</Name>
            <Tests>
              <string>var:Patient.SP condition:less constant:85</string>
            </Tests>
          </DecisionCondition>
          <DecisionAction>
            <Name>Action1</Name>
            <Actions>
              <string>status:"WARN:PLAYER:OXYGEN:LOW"</string>
