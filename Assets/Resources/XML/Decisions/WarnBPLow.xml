<?xml version="1.0" encoding="utf-8"?>
<DecisionMap xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>WarnBPLow</Name>
  <StartState>Watching</StartState>
  <Interval>1</Interval>
  <DecisionStates>
    <DecisionState>
      <Name>Watching</Name>
      <Transitions>
        <DecisionStateTransition>
          <DecisionCondition>
            <Name>TEST DONT WARN IF DEAD</Name>
            <Tests>
              <string>var:Patient.BP_SYS condition:less constant:5</string>
            </Tests>
          </DecisionCondition>
          <SuccessState>Recovering</SuccessState>
        </DecisionStateTransition>
        <DecisionStateTransition>
          <DecisionCondition>
            <Name>TEST BP_SYS</Name>
            <Tests>
              <string>var:Patient.BP_SYS condition:less constant:75</string>
            </Tests>
          </DecisionCondition>
          <DecisionAction>
            <Name>Action1</Name>
            <Actions>
              <string>status:"WARN:PLAYER:BLOODPRESSURE:LOW"</string>
            </Actions>
          </DecisionAction>
          <SuccessState>Recovering</SuccessState>
        </DecisionStateTransition>
      </Transitions>
    </DecisionState>
    <DecisionState>
      <Name>Recovering</Name>
      <Transitions>
        <DecisionStateTransition>
          <DecisionCondition>
            <Name>TEST BP_SYS</Name>
            <Tests>
              <string>var:Patient.BP_SYS condition:greater constant:80</string>
            </Tests>
          </DecisionCondition>
          <DecisionAction>
            <Name>Action1</Name>
            <Actions />
          </DecisionAction>
          <SuccessState>Watching</SuccessState>
        </DecisionStateTransition>
      </Transitions>
    </DecisionState>
  </DecisionStates>
  <Finished>false</Finished>
  <CheckTime>0</CheckTime>
</DecisionMap>