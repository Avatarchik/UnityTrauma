            <Score>10</Score>
            <ScoreMax>10</ScoreMax>
          </AssessmentItem>
          <AssessmentItem>
            <InteractionName>CALL:IR</InteractionName>
			<Note>Order Patient to the IR for Bleeding</Note>			
            <Score>10</Score>
            <ScoreMax>10</ScoreMax>
          </AssessmentItem>
          <AssessmentItem>
            <InteractionName>EXAM:PELVIC</InteractionName>
			<Note>Examine Pelvis</Note>			
            <Score>10</Score>
            <ScoreMax>10</ScoreMax>
          </AssessmentItem>
          <AssessmentItem>
            <InteractionName>PELVIC:WRAP</InteractionName>
			<Note>Perform Pelvic Wrap</Note>			
            <Score>10</Score>
            <ScoreMax>10</ScoreMax>
          </AssessmentItem>
          <AssessmentItem>
            <InteractionName>SCHEDULE:OR:LAPAROTOMY</InteractionName>
			<Note>Order Patient Laparotomy</Note>			
            <Score>10</Score>
            <ScoreMax>10</ScoreMax>
          </AssessmentItem>
        </Items>
</AssessmentList>			

