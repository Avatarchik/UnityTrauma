            <Area>
              <name>XrayButtonAbNormal</name>
              <skin>ApplicationSkin</skin>
              <style>dialog.xray.abnormal.area</style>
              <anchor>relative</anchor>
              <Elements>
                <Label>
                  <name>xrayButtonLine</name>
                  <skin>ApplicationSkin</skin>
                  <style>dialog.fast.line02</style>
                </Label>
                <Button>
                  <name>xray.button.abnormal</name>
                  <skin>ApplicationSkin</skin>
                  <style>button.xray.abnormal</style>
                  <text>NO BLOOD</text>
				  <interactMsg>PLAYER:XRAY:CHEST:FALSE</interactMsg>					
 				  <closeWindow>true</closeWindow>			
               </Button>
                <Label>
                  <name>xray.button.normal.text</name>
                  <skin>ApplicationSkin</skin>
                  <style>dialog.xray.text</style>
                  <text>There is no blood</text>
                </Label>
              </Elements>
            </Area>
			Xray Button abnormal-->
            <!-- Xray Button abnormal-->
            
          </Elements>
        </Area>
        <!-- Xray Content Lower Area-->
        
        
      </Elements>
    </GUIScreen>
  </Screens>
</ScreenInfo>